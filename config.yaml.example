version: 1

sources:

  - sharepoint_dev:
      class: Sharepoint
      credentials:
        site: "<TENANT_NAME>.sharepoint.com"
        username: "<USERNAME>"
        password: "<PASSWORD>"

  - supermetrics:
    class: Supermetrics
    credentials:
      api_key: "<API_KEY>",
      user: "<USER>",
  
  - exchange_rates_dev:
      class: ExchangeRates
      credentials:
        api_key: "<API_KEY>"